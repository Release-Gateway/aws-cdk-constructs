{
    "name": "@release-gateway/aws-cdk-constructs",
    "version": "0.0.0",
    "description": "Enterprise ready CDK constructs that are CIS 1.4 and NIST800-53 Rev.5 standards compliant",
    "main": "src/index.js",
    "types": "src/index.d.ts",
    "scripts": {
        "test": "jest",
        "mutation-test": "npx stryker run",
        "build": "jsii",
        "build:watch": "jsii --watch",
        "lint": "eslint --ext .ts .",
        "semantic-release": "semantic-release",
        "prepackage": "rm -rf ./dist",
        "package": "jsii-pacmak",
        "publish": "publib-npm"
    },
    "keywords": [
        "cdk",
        "awscdk",
        "aws-cdk",
        "cloudformation",
        "enterprise",
        "cis",
        "nist",
        "policy-as-code",
        "compliance",
        "validator",
        "cdk-validator-cfnguard"
    ],
    "bundledDependencies": [
        "lodash",
        "zod"
    ],
    "author": "Christopher Imrie",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Release-Gateway/aws-cdk-constructs.git"
    },
    "publishConfig": {
        "access": "public",
        "tag": "latest"
    },
    "devDependencies": {
        "@aws-cdk/aws-apigatewayv2-alpha": "2.114.1-alpha.0",
        "@aws-cdk/aws-apigatewayv2-integrations-alpha": "2.114.1-alpha.0",
        "@cdklabs/cdk-validator-cfnguard": "0.0.60",
        "@semantic-release/changelog": "^6.0.3",
        "@semantic-release/exec": "^6.0.3",
        "@semantic-release/git": "^10.0.1",
        "@semantic-release/github": "^9.2.3",
        "@stryker-mutator/core": "^9.3.0",
        "@stryker-mutator/jest-runner": "^9.3.0",
        "@tsconfig/recommended": "^1.0.11",
        "@types/jest": "^29.5.14",
        "@types/lodash": "^4.17.20",
        "@types/node": "^20.19.24",
        "@typescript-eslint/eslint-plugin": "^8.46.2",
        "@typescript-eslint/parser": "^8.46.2",
        "aws-cdk-lib": "2.221.1",
        "constructs": "10.4.2",
        "esbuild": "^0.25.11",
        "eslint": "^8.57.1",
        "eslint-config-prettier": "^9.1.2",
        "eslint-plugin-prettier": "^5.5.4",
        "jsii": "^5.9.10",
        "jsii-pacmak": "^1.118.0",
        "prettier": "^3.6.2",
        "prettier-2": "npm:prettier@^2",
        "publib": "^0.2.1025",
        "semantic-release": "^22.0.12",
        "ts-jest": "^29.4.5",
        "typedoc": "^0.28.14",
        "typedoc-plugin-markdown": "^4.9.0",
        "typescript": "^5.9.3"
    },
    "peerDependencies": {
        "@aws-cdk/aws-apigatewayv2-alpha": "^2.114.1-alpha.0",
        "@aws-cdk/aws-apigatewayv2-integrations-alpha": "^2.114.1-alpha.0",
        "@cdklabs/cdk-validator-cfnguard": "^0.0.60",
        "aws-cdk-lib": "^2.221.1",
        "constructs": "^10.4.2"
    },
    "dependencies": {
        "lodash": "^4.17.21",
        "zod": "^3.25.76"
    },
    "release": {
        "branches": [
            "main",
            {
                "name": "alpha",
                "prerelease": true
            }
        ],
        "plugins": [
            "@semantic-release/commit-analyzer",
            "@semantic-release/release-notes-generator",
            "@semantic-release/changelog",
            [
                "@semantic-release/npm",
                {
                    "npmPublish": false
                }
            ],
            [
                "@semantic-release/exec",
                {
                    "verifyConditionsCmd": "npm run build",
                    "prepareCmd": "npm run package",
                    "publishCmd": "npm run publish"
                }
            ],
            [
                "@semantic-release/github",
                {
                    "assets": [
                        {
                            "path": "dist/js/*.tgz",
                            "label": "NPM Package"
                        },
                        {
                            "path": "dist/python/*.tar.gz",
                            "label": "PyPI Source Distribution"
                        },
                        {
                            "path": "dist/python/*.whl",
                            "label": "PyPI Built Distribution"
                        }
                    ]
                }
            ]
        ]
    },
    "bugs": {
        "url": "https://github.com/Release-Gateway/aws-cdk-constructs/issues"
    },
    "homepage": "https://github.com/Release-Gateway/aws-cdk-constructs#readme",
    "stability": "stable",
    "jsii": {
        "excludeTypescript": [
            "test/**"
        ],
        "outdir": "dist",
        "versionFormat": "short",
        "targets": {
            "python": {
                "distName": "release-gateway.aws-cdk-constructs",
                "module": "release_gateway.aws_cdk_constructs"
            }
        }
    },
    "bundleDependencies": [
        "lodash",
        "zod"
    ]
}
